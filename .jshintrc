{
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "forin": true,
  "freeze": true,
  "immed": true,
  "indent": 2,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonew": true,
  "strict": true,
  "quotmark": "single",
  "maxdepth": 3,
  "trailing": true,
  /* TODO: Get maxstatements to 15 */
  "maxstatements": 25,
  "maxlen": 120,
  "eqnull": true,
  "funcscope": true,
  "undef": true,
  "unused": true,
  "node": true
}
